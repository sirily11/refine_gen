{% extends "antd/function.jinja" %}
{% block import -%}
import { Create } from "@pankod/refine-antd";
{% endblock -%}

{% block selections %}
      {% for selection in selections -%}
                {{ selection }}
        {% endfor -%}
{% endblock %}

{% block setup %}
    const { formProps, saveButtonProps } = useForm();
{% endblock %}

{% block fields %}
    return (
        //@ts-ignore
        <Create saveButtonProps={saveButtonProps}>
            <Form {...formProps} layout="vertical" >
                {% for field in fields -%}
                    {{ field }}
                {% endfor -%}
            </Form>
        </Create>
    );
{% endblock %}